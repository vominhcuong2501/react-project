{
  "stylelint.validate": ["css", "scss"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.sortImports": false,
    "source.organizeImports": true,
    "source.fixAll": true,
    "source.sortMembers": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "autofocus",
    "camelcase",
    "crumblist",
    "deduping",
    "flowtype",
    "FOOTERYEAR",
    "formik",
    "hookform",
    "largedesktop",
    "lastest",
    "leadform",
    "linebreak",
    "maxage",
    "mediumdesktop",
    "mediumphone",
    "multilines",
    "oneibc",
    "oneibccom",
    "oneline",
    "paren",
    "reduxjs",
    "roachjs",
    "roboto",
    "scrolldown",
    "smalldesktop",
    "smallphone",
    "stylelint",
    "swiper",
    "Swipper",
    "tailwindcss",
    "Tiktok",
    "Touchs",
    "xlargedesktop",
    "xsmallphone"
  ]
}
